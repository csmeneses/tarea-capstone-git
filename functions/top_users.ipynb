    "def top_10_users(tweets, users):\n",
    "    tweets_users = pd.merge(tweets, users, how=\"inner\", on=\"userId\")\n",
    "    return list(tweets_users.groupby(by=\"username\").count().sort_values(by=\"tweetId\", ascending=False).index.values[:10])"
